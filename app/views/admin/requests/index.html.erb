<center><h1><%= t ".list_request" %></h1></center>
<div class="col-md-10 col-md-offset-1">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col" class="text-center">#</th>
        <th scope="col" class="text-center"><%= t ".book_name" %></th>
        <th scope="col" class="text-center"><%= t ".user_request" %></th>
        <th scope="col" class="text-center"><%= t ".status" %></th>
        <th scope="col" class="text-center"><%= t ".request_at" %></th>
        <th scope="col" class="text-center"><%= t ".action" %></th>
      </tr>
    </thead>
    <tbody><%= render partial: "request", collection: @requests, as: :request %></tbody>
  </table>
</div>
